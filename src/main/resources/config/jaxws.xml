<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:jaxws="http://cxf.apache.org/jaxws"
       xsi:schemaLocation=" http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://cxf.apache.org/jaxws
        http://cxf.apache.org/schemas/jaxws.xsd">

    <jaxws:endpoint id="TeamSoapServiceWS"       implementor="#teamSoapServiceImpl"       address="/TeamSoapService"/>
    <jaxws:endpoint id="CitySoapServiceWS"       implementor="#citySoapServiceImpl"       address="/CitySoapService"/>
    <jaxws:endpoint id="WrestlerSoapServiceWS"   implementor="#wrestlerSoapServiceImpl"   address="/WrestlerSoapService"/>
    <jaxws:endpoint id="JudgeSoapServiceWS"      implementor="#judgeSoapServiceImpl"      address="/JudgeSoapService"/>
    <jaxws:endpoint id="TournamentSoapServiceWS" implementor="#tournamentSoapServiceImpl" address="/TournamentSoapService"/>

</beans>